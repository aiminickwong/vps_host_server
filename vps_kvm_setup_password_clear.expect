#!/usr/bin/expect -f
set timeout -1
set name [lindex $argv 0]
set stty_init raw
spawn /root/cpaneldirect/chntpw /vz/mounts/${name}/Windows/System32/config/SAM
#match_max 100000
expect "Select: "
send "1\n"
expect "Write hive files? "
send "y\n"
expect eof
spawn /root/cpaneldirect/chntpw /vz/mounts/${name}/Windows/System32/config/SAM
#match_max 100000
expect "Select: "
send "4\n"
expect "Write hive files? "
send "y\n"
expect eof
